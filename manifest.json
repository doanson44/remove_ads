{
  "name": "Remove Phimmoi Ads",
  "description": "Remove Phimmoi Ads",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "webRequest"
  ],
  "browser_action": {
      "default_title": "Remove Ads.",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*.phimmoizz.net/*"],
      "all_frames": true,
      "css": ["phimmoizz.css"],
      "js": ["phimmoizz.js"]
    }
  ],
  "manifest_version": 2
}
